<h1>Nosotros</h1>

<table class="table table-striped table-bordered table">
<thead>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Telefono</th>
    <th>Mail</th>
    <th>Web</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
</thead><tbody>
<% @owners.each do |owner| %>
  <tr>
    <td><%= owner.name %></td>
    <td><%= owner.lastname %></td>
    <td><%= owner.phone %></td>
    <td><%= owner.mail %></td>
    <td><%= owner.web %></td>
    <td><%= link_to 'Ver', owner %></td>
    <td><%= link_to 'Editar', edit_owner_path(owner) %></td>
    <td><%= link_to 'Eliminar', owner, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %></tbody>
</table>


<%= link_to 'Crear Tecnico', new_owner_path, class: 'btn btn-primary' %>
